List a = Nil | Cons { car :: a, cdr :: List a }

main :: Int
main = 1

g :: forall a b. a -> { x :: a, y :: Int }
g x = { y = 1, x = x }

h :: forall a. a -> List a
h x = Cons { car = x, cdr = Cons { car = x, cdr = Nil } }

